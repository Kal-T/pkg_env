@import '../../../assets/scss/variables';
.mystyle {
    width: 100%;
    padding: 25px;
    background-color: coral;
    color: white;
    font-size: 25px;
    box-sizing: border-box;
    display: none;
}

.menu {
    border: 1px solid #c9c9c9;
    color: black;
    height:80%;
    
}

.show {
    display: block;
}

.hide {
    display: hide;
}

#objInfo{
    width: 100%;
}

.objInfoHeader {
    margin-left: 10px;
    font-weight: bold;
    color: white;
    text-align: left;
}
.ant-collapse-header {
    background-color: #3baae3;
    padding : 0px 16px!important;
    height: 25px;
}
.object-info-layout{
    padding:4px 4px;
}
.ant-form-item{
    margin-bottom: 0px!important;
}
.ant-collapse-content.ant-collapse-content-active{
    padding:0px 16px;
}
.ant-collapse-content > .ant-collapse-content-box {
    padding : 5px!important;
}
.ant-col > label#usrName{
    padding-top : 10px;
}
.timeout-warn-input > .ant-input-group > .ant-input{
    padding : 5px;
}
.timeout-warn-input > .ant-input-group > .ant-input-group-addon{
    padding : 0px 5px;
}
.ant-form label{
    font-size: $sm-font-size;
}
.form-item-desc .ant-form-item-control {
    margin-left: 16px;
}
.form-item-id .ant-form-item-control {
    margin-left: 17px;
}