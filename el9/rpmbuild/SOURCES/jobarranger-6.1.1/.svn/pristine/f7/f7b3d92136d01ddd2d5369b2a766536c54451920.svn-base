.sidebar-overlay {
    position: absolute;
    top: 0;
    left: 0;
    height: 100%;
    width: 100%;
    opacity: 0.4;
    background: #e1e1e1;
  }

  .graph-overlay {
    position: absolute;
    top: 0;
    left: 0;
    height: 10000px;
    width: 5000px;
    opacity: 0.3;
    background: #e1e1e1;
  }

  .ant-select-tree-treenode {
    padding: 0px !important;
}

  .tree-combo-row {
    display: flex;
    justify-content: space-between;

    .font-bold {
      font-weight: bold !important;
      color: rgba(0, 0, 0, 1) !important;
    }

    .border-top {
      border-top: 1px solid #b5aeae;
    }

    .border-right {
      border-right: 1px solid #b5aeae;
    }

    .border-side {
      border-right: 1px solid #b5aeae;
      border-left: 1px solid #b5aeae;
    }

    .combo-tree-item {
      padding: 3px 6px;
    }

    .w-100 {
      width: 100% !important;
    }

    .w-50 {
      width: 50% !important;
    }

}